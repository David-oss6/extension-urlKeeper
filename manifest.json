{
    "manifest_version": 3,
    "name": "urlKeeper",
    "version": "1.0",
    "description": "Storage url and data",
    "icons": {
      "128": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "permissions": ["tabs","storage","clipboardRead"],
    "host_permissions": ["<all_urls>"]
  }
  